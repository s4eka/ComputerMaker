@model IEnumerable<ComputerMaker.Models.Processor>

@{
    ViewBag.Title = "GetProcessorList";
}

<h2>GetProcessorList</h2>

<p>
    @Html.ActionLink("Create New", "CreateNewProcessor")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Socet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoryType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxMemoryFrequency)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IntegratedGraphics)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Power)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Socet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoryType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxMemoryFrequency)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IntegratedGraphics)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Power)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
